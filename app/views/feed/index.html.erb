<% @feed.each do |feed| %>
     <h3><%= feed.title %></h3>
     <p><%= feed.content %></p>
     <hr>
     <h5>Comments</h5>
     <%= form_with(model: @comment, url: comments_path ) do |f| %>
          
          <%= f.text_area :body %><br>
          <%= f.submit "Comment", data: { "disable-with": "Saving..." } %>
     <% end %>
     <% feed.comments.each do |comment| %>
     <p><%= comment.body %></p>
     <% end %>
     <hr>
<% end %>